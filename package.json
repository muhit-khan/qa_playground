{
  "name": "a1qa-wdio-framework",
  "type": "module",
  "scripts": {
    "chrome.mocha": "wdio run ./configs/chrome.mocha.conf.js",
    "test:main:page": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/MainPageTest.js",
    "test:dynamic:button": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/DynamicButtonTest.js",
    "test:class:attribute": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/ClassAttributePageTest.js",
    "test:load:delay": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/LoadDelaysTest.js",
    "test:ajax:data": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/AJAXDataTest.js",
    "test:client:side:delay": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/ClientSideDelayTest.js",
    "test:click": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/ClickTest.js",
    "test:text:input": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/TextInputTest.js",
    "test:progress:bar": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/ProgressBarTest.js"
  },
  "dependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/register": "^7.23.7",
    "@wdio/allure-reporter": "^8.34.0",
    "@wdio/cli": "^9.2.1",
    "@wdio/local-runner": "^9.2.1",
    "@wdio/mocha-framework": "^8.35.0",
    "@wdio/spec-reporter": "^8.32.4",
    "chai": "^5.1.0",
    "fs-extra": "^11.2.0",
    "winston": "^3.13.0"
  }
}